{
  "ignorePaths": [
    "**/node_modules/**",
    "**/vscode-extension/**",
    "**/.git/**",
    "**/.pnpm-lock.json",
    ".vscode",
    "package-lock.json",
    "megalinter-reports"
  ],
  "allowCompoundWords": true,
  "version": "0.2",
  "language": "en,fr",
  "caseSensitive": false,
  "languageSettings": [
    {
      "languageId": "*",
      "caseSensitive": false
    }
  ],
  "words": [
    "CLIENTID",
    "csocket",
    "GUIRRIEC",
    "NOPASSWD",
    "Posix",
    "certifi",
    "cffi",
    "chardet",
    "clib",
    "hadolint",
    "idna",
    "inputrc",
    "jmespath",
    "netifaces",
    "nosemgrep",
    "posix",
    "pylintrc",
    "pytz",
    "traefik",
    "venv"
  ]
}
